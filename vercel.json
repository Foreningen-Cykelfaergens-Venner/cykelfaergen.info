{
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.7.1"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    },
    {
      "src": "/css/style.css",
      "dest": "/api/styles/style.css"
    },
    {
      "src": "/js/script.js",
      "dest": "/api/js/final.min.js"
    }
  ]
}